{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "latex",
      "scope": "source.tex",
      "file-types": [
        "tex",
        "aux",
        "sty",
        "cls"
      ],
      "injection-regex": "^latex$"
    }
  ],
  "metadata": {
    "version": "0.4.0",
    "license": "MIT",
    "description": "Latex grammar for tree-sitter",
    "authors": [
      {
        "name": "Patrick FÃ¶rster",
        "email": "patrick.foerster@outlook.de"
      }
    ],
    "links": {
      "repository": "https://github.com/latex-lsp/tree-sitter-latex"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
