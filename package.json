    "test": "tree-sitter test",
    "test:update": "tree-sitter test --update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/latex-lsp/tree-sitter-latex.git"
  },
  "keywords": [
    "tree-sitter",
    "latex",
    "parser"
  ],
