  "license": "MIT",
  "bugs": {
    "url": "https://github.com/latex-lsp/tree-sitter-latex/issues"
  },
  "homepage": "https://github.com/latex-lsp/tree-sitter-latex#readme",
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "tree-sitter-cli": "0.20.6"
  },
  "tree-sitter": [
    {
      "scope": "source.tex",
      "file-types": [
        "tex",
        "sty",
        "cls",
        "aux"
      ]
